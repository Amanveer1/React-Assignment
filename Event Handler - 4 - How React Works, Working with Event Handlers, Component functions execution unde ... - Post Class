import React, { Component } from 'react';

class App extends Component {
  handleDoubleClick = () => {
    console.log('I was double clicked');
  };

  render() {
    return (
      <div>
        <button id="dblclick-btn" onDoubleClick={this.handleDoubleClick}>
          Double Click Me
        </button>
      </div>
    );
  }
}

export default App;

